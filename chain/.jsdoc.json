{
    "tags": {
      "allowUnknownTags": true,
      "dictionaries": ["jsdoc"]
    },
    "source": {
      "include": ["src", "package.json", "README.md"],
      "includePattern": ".js$",
      "excludePattern": "(node_modules/|docs)"
    },
    "plugins": [
      "plugins/markdown"
    ],
    "templates": {
        "referenceTitle": "@leofcoin/chain",
        "disableSort": false,
        "collapse": true,
        "resources": {
          "github": "https://github.com/arteontoken/monorepo/tree/main/chain"
        }
    },
    "opts": {
        "destination": "./docs/",
        "encoding": "utf8",
        "private": true,
        "recurse": true,
        "template": "./node_modules/jsdoc-template"
    }
}
