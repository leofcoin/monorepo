{
  "name": "@leofcoin/chain",
  "version": "1.3.7",
  "description": "Official javascript implementation",
  "main": "./dist/node.js",
  "module": "./dist/chain.esm",
  "scripts": {
    "np": "np",
    "lint": "eslint src --fix",
    "c": "rollup -c",
    "w": "rollup -c -w",
    "docs": "sh ./node_modules/.bin/esdoc src/chain.js",
    "build": "npm run c && webpack && cp ./dist/*browser.js ./demo && cp -r ./dist/browser/workers/ ./demo",
    "demo": "jsproject --serve ./demo --port 5478 --open demo",
    "test": "node test --openssl-legacy-provider",
    "pack": "webpack"
  },
  "np": {
    "yarn": false,
    "contents": "."
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-strip": "^2.1.0",
    "@vandeurenglenn/debug": "^1.0.0",
    "crypto-browserify": "^3.12.0",
    "eslint": "^8.28.0",
    "eslint-plugin-unicorn": "^45.0.0",
    "open": "^8.4.0",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "rollup": "^2.70.1",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-terser": "^7.0.2",
    "stream-browserify": "^3.0.0",
    "tape": "^5.5.2",
    "terser": "^5.16.0",
    "vm-browserify": "^1.1.2",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "@ethersproject/bignumber": "^5.6.0",
    "@ethersproject/units": "^5.6.0",
