{
  "name": "@leofcoin/chain",
  "version": "1.4.11",
  "description": "Official javascript implementation",
  "main": "./dist/node.js",
  "module": "./dist/chain.esm",
  "exports": {
    "./node": "./src/node.js",
    "./chain": "./src/chain.js"
  },
  "type": "module",
  "scripts": {
    "np": "np",
    "lint": "eslint src --fix",
    "c": "rollup -c",
    "w": "rollup -c -w",
    "docs": "sh ./node_modules/.bin/esdoc src/chain.js",
    "workers": "cp ./../workers/src/** ./workers",
    "build": "npm run c && webpack && cp ./dist/*browser.js ./demo && cp -r ./dist/browser/workers/ ./demo",
    "demo": "jsproject --serve ./demo --port 5478 --open demo",
    "test": "node --openssl-legacy-provider test",
    "pack": "webpack"
  },
  "np": {
    "yarn": false,
    "contents": "."
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/bs58check": "^2.1.0",
    "@types/pako": "^2.0.0",
    "@types/randombytes": "^2.0.0",
    "@types/varint": "^6.0.1",
    "@vandeurenglenn/debug": "^1.0.0",
    "eslint": "^8.28.0",
    "eslint-plugin-unicorn": "^45.0.0",
    "open": "^8.4.0",
    "tape": "^5.5.2",
    "tslib": "^2.4.1"
  },
  "dependencies": {
    "@leofcoin/addresses": "^1.0.4",
    "@leofcoin/codec-format-interface": "^1.5.0",
    "@leofcoin/lib": "^1.0.11",
    "@leofcoin/messages": "^1.2.0",
    "@leofcoin/multi-wallet": "^2.1.1",
    "@leofcoin/networks": "^1.0.0",
    "@leofcoin/peernet": "^0.17.0",
    "@leofcoin/storage": "^3.0.6",
    "@leofcoin/utils": "^1.1.4",
    "@leofcoin/workers": "^1.3.5",
    "@vandeurenglenn/base32": "^1.1.0",
    "@vandeurenglenn/easy-worker": "^1.0.2",
    "@vandeurenglenn/queue": "^1.0.0",
    "chalk": "^4.0.0",
    "pako": "^2.0.4"
  },
  "eslintConfig": {
    "env": {
      "es2022": true
    },
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "extends": "plugin:unicorn/recommended"
  }
}
