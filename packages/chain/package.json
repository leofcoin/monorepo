    "@leofcoin/networks": "^1.0.0",
    "@leofcoin/peernet": "^1.1.37",
    "@leofcoin/peernet-swarm": "^1.0.0",
    "@leofcoin/storage": "^3.0.6",
    "@leofcoin/utils": "^1.1.4",
    "@leofcoin/workers": "^1.3.13",
    "@netpeer/p2pt-swarm": "^1.3.3",
    "@vandeurenglenn/base32": "^1.1.0",
    "@vandeurenglenn/easy-worker": "^1.0.2",
    "@vandeurenglenn/queue": "^1.0.0",
    "chalk": "^4.0.0",
    "p-queue": "^7.3.4",
    "p2pt": "^1.5.1",
    "pako": "^2.0.4",
    "simple-peer": "^9.11.1"
  },
  "eslintConfig": {
    "env": {
      "es2022": true
    },
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "extends": "plugin:unicorn/recommended"
  }
