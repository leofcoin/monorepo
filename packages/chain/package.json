{
  "name": "@leofcoin/chain",
  "version": "1.4.86",
  "description": "Official javascript implementation",
  "exports": {
    "./node": "./exports/node.js",
    "./chain": "./exports/chain.js",
    "./workers/machine-worker": "./exports/workers/machine-worker.js"
  },
  "type": "module",
  "files": [
    "./exports",
    "LICENSE.txt"
  ],
  "scripts": {
    "np": "np",
    "watch": "rollup -c -w",
    "lint": "eslint src --fix",
    "w": "rollup -c -w",
    "docs": "sh ./node_modules/.bin/esdoc src/chain.js",
    "workers": "cp ./../workers/src/** ./workers",
    "demo": "jsproject --serve ./ --port 5478 --open demo",
    "test": "node --openssl-legacy-provider test",
    "pack": "webpack",
    "build": "wireit"
  },
  "wireit": {
    "build": {
      "command": "rollup -c --silent error --cache true"
    }
  },
  "np": {
    "yarn": false,
    "contents": "."
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-typescript": "^11.1.1",
    "@types/bs58check": "^2.1.0",
    "@types/pako": "^2.0.0",
    "@types/randombytes": "^2.0.0",
    "@types/varint": "^6.0.1",
    "@vandeurenglenn/debug": "^1.0.0",
    "eslint": "^8.28.0",
    "eslint-plugin-unicorn": "^45.0.0",
    "open": "^8.4.0",
    "rimraf": "^4.4.0",
    "rollup": "^3.25.1",
    "rollup-plugin-modify": "^3.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "tape": "^5.5.2",
    "tslib": "^2.5.3",
    "wireit": "^0.9.5"
  },
  "dependencies": {
    "@leofcoin/addresses": "^1.0.4",
    "@leofcoin/codec-format-interface": "^1.6.14",
